<h1>Edit Trip</h1>


<form action="/trips/<%=@trips.id %>" method="post">
  <input type="hidden" name="_method" value="patch">
  <label>City: </label>
  <input type="text" name="trip[city]"  value="<%=@trips.city%>">
  <label>Country: </label>
  <input type="text" name="trip[country]"  value="<%=@trips.country%>">
  <label>Dates: </label>
  <input type="text" name="trip[date]"  value="<%=@trips.date%>">
  <input type="submit">
</form>